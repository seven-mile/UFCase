  <Target Name="ProcessWinMD" BeforeTargets="ClCompile">
    <ItemGroup>
      <_WinMDPaths Include="$(SolutionDir)$(Platform)\$(Configuration)\UFCase\UFCase.Host.Manifest.winmd" />
      <_WinMDPaths Include="$(SolutionDir)$(Platform)\$(Configuration)\UFCase\UFCase.Isolation.winmd" />
    </ItemGroup>
    <PropertyGroup>
      <WinMDToolsPath Condition="'$(WinMDToolsPath)'==''">"$(WindowsSdkDir)bin\$(TargetPlatformVersion)\$(Platform)"</WinMDToolsPath>
      <_ProxyStubWorkspace>$(ProjectDir)$(IntDir)ProxyStub</_ProxyStubWorkspace>
    </PropertyGroup>
    <ItemGroup>
      <WinMDFiles Include="@(_WinMDPaths)">
        <IdlFileName>$([System.IO.Path]::GetFileNameWithoutExtension('%(Identity)')).idl</IdlFileName>
        <DllDataFileName>$([System.IO.Path]::GetFileNameWithoutExtension('%(Identity)'))_d.c</DllDataFileName>
      </WinMDFiles>
    </ItemGroup>
    <!-- Create the workspace -->
    <MakeDir Directories="$(_ProxyStubWorkspace)" Condition="!Exists('$(_ProxyStubWorkspace)')" />
    <!-- Verify the input files -->
    <Error Condition="!Exists('%(WinMDFiles.Identity)')" Text="WinMD file not found: %(WinMDFiles.Identity)" />
    <!-- Generate IDL from WinMD -->
    <Exec Command="WinMdIdl.exe /outdir:&quot;$(_ProxyStubWorkspace)&quot; &quot;%(WinMDFiles.Identity)&quot;" WorkingDirectory="$(MSBuildProjectDirectory)" />
    <!-- Generate Proxy/Stub from IDL -->
    <Exec Command="MIdl.exe /env $(Platform) /winrt /ns_prefix /dlldata &quot;%(WinMDFiles.DllDataFileName)&quot; /metadata_dir &quot;$(WindowsSDK_UnionMetadataPath)&quot; /I &quot;$(_ProxyStubWorkspace)&quot; /out &quot;$(MSBuildProjectDirectory)&quot; &quot;$(_ProxyStubWorkspace)\%(WinMDFiles.IdlFileName)&quot;" WorkingDirectory="$(MSBuildProjectDirectory)" />
  </Target>
